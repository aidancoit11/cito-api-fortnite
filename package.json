{
  "name": "fortnite-competitive-api",
  "version": "1.0.0",
  "description": "Comprehensive Fortnite competitive esports API with player stats, tournament data, earnings, and organization tracking",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "generate-auth": "tsx src/scripts/generate-device-auth.ts",
    "test-epic": "tsx src/scripts/test-epic-api.ts",
    "email:auth": "tsx src/scripts/check-outlook-graph.ts auth",
    "email:check": "tsx src/scripts/check-outlook-graph.ts",
    "setup-email": "tsx src/scripts/setup-temp-email.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "jobs:token-refresh": "tsx src/jobs/token-refresh.ts",
    "jobs:all": "tsx src/jobs/index.ts"
  },
  "keywords": [
    "fortnite",
    "esports",
    "api",
    "competitive",
    "tournament",
    "stats",
    "epic-games"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@azure/msal-node": "^3.8.4",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.45.4",
    "@techstark/opencv-js": "^4.12.0-release.1",
    "2captcha": "^3.0.5-2",
    "axios": "^1.7.7",
    "canvas": "^3.2.0",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "https-proxy-agent": "^7.0.6",
    "imap": "^0.8.19",
    "jimp": "^1.6.0",
    "mailparser": "^3.9.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.34.5",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/express": "^5.0.0",
    "@types/imap": "^0.8.43",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^22.9.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitest/coverage-v8": "^2.1.4",
    "eslint": "^9.14.0",
    "playwright": "^1.57.0",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
